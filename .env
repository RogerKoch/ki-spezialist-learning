# =================================
# KI-SPEZIALIST LEARNING - API KEYS
# =================================
# WICHTIG: Kopieren Sie diese Datei zu .env und fügen Sie Ihre echten API-Keys ein
# .env wird von Git ignoriert und ist sicher

# === OPENAI API ===
OPENAI_API_KEY=sk-proj-your-openai-key-here
OPENAI_ORG_ID=org-your-organization-id-here

# === ANTHROPIC (CLAUDE) ===
ANTHROPIC_API_KEY=sk-ant-api03-your-anthropic-key-here

# === GOOGLE (GEMINI) ===
GOOGLE_API_KEY=your-google-ai-api-key-here

# === WEATHER API (für Übungen) ===
OPENWEATHER_API_KEY=your-openweathermap-key-here

# === OPTIONAL APIs ===
COHERE_API_KEY=your-cohere-key-here
HUGGINGFACE_API_KEY=hf_your-huggingface-key-here

# === DATABASE KONFIGURATION ===
DATABASE_URL=sqlite:///data/learning_progress.db

# === COST TRACKING ===
MONTHLY_BUDGET_USD=50.00
COST_ALERT_THRESHOLD=0.80
ENABLE_COST_TRACKING=true

# === RATE LIMITING ===
DEFAULT_RATE_LIMIT_RPM=60
OPENAI_RATE_LIMIT_RPM=500
ANTHROPIC_RATE_LIMIT_RPM=50

# === LOGGING ===
LOG_LEVEL=INFO
LOG_FILE=logs/ki-learning.log

# === DEVELOPMENT ===
DEBUG_MODE=false
ENVIRONMENT=development